{
    "version":2,
    "builds":[
        {
            "src": "src/**/*.js",
            "use": "@vercel/node@2.5.1"
        }
    ],
    "routes":[
        {
            "src":"/api/(.*)",
            "dest":"src/main.js"
        },
        {
            "handle":"filesystem"
        },
        {
            "src":"/(.*)",
            "dest":"src/main.js"
        }
    ]
}
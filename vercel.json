{
    "version": 2,
    "builds": [
      {
        "src": "src/**/*.js",
        "use": "@vercel/node@2.5.1"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "src/routes/api/apiRouter.js"
      },
      {
        "src": "/views/(.*)",
        "dest": "views/$1"
      },
      {
        "src": "/(.*)",
        "dest": "src/main.js"
      }
    ]
  }
  